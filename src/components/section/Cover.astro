---
import { Image } from "astro:assets";

interface Props {
  imgSource: any;
  position: string;
}
const { imgSource, position = "object-center" } = Astro.props;
---

<div>
  <Image
    src={imgSource}
    alt=""
    inferSize={true}
    class=`w-max max-h-[400px] object-cover ${position}`
  />
</div>
